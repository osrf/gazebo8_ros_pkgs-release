From c951895fd04941af18322157f9890c15970eec55 Mon Sep 17 00:00:00 2001
From: Jose Luis Rivero <jrivero@osrfoundation.org>
Date: Wed, 7 Jun 2017 00:24:02 +0200
Subject: [PATCH] Use gazebo8 instead of gazebo8 in package.xml

---
 package.xml | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/package.xml b/package.xml
index 1910235..8aabe61 100644
--- a/package.xml
+++ b/package.xml
@@ -19,6 +19,6 @@
 
   <buildtool_depend>catkin</buildtool_depend>
 
-  <build_export_depend>libgazebo7-dev</build_export_depend>
-  <exec_depend>gazebo7</exec_depend>
+  <build_export_depend>libgazebo8-dev</build_export_depend>
+  <exec_depend>gazebo8</exec_depend>
 </package>
-- 
